{
  "name": "nx-go-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "packages/nx-go-e2e/src",
  "implicitDependencies": ["nx-go"],
  "targets": {
    "e2e": {
      "options": {
        "passWithNoTests": true,
        "runInBand": true
      },
      "dependsOn": ["^build"],
      "configurations": {
        "ci": {
          "ci": true,
          "coverage": true
        }
      }
    }
  }
}
